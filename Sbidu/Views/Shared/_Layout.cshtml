<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Sbidu - Bid And Auction HTML Template</title>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/4.5.3/css/bootstrap.min.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/animate.css/4.1.1/animate.min.css">
    <link rel="stylesheet" href="~/font/flaticon.css" />
    <link rel="stylesheet" href="~/css/style.css">
    <link rel="shortcut icon" href="~/uploads/favicon-logo.png" type="image/x-icon">
    @RenderSection("Styles", required: false)
    <script src="https://kit.fontawesome.com/5f8f97e3fd.js" crossorigin="anonymous"></script>
</head>

<body>

    <section id="loader">
        <div class="img-loader">
        </div>
    </section>

    <header>
        <vc:header-component></vc:header-component>
        <!-- Navbar -->
        <div class="header-bottom">
            <nav class="navbar navbar-expand-lg">
                <div class="container">
                    <div class="logo">
                        <a class="navbar-brand" asp-controller="home" asp-action="index">
                            <img src="~/uploads/logo.png" alt="logo">
                        </a>
                        <button class="navbar-toggler" type="button" data-toggle="collapse"
                                data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent"
                                aria-expanded="false" aria-label="Toggle navigation">
                            <i class="fas fa-bars navbar-toggler-icon"></i>
                        </button>
                    </div>

                    <div class="collapse navbar-collapse justify-content-end" id="navbarSupportedContent">
                        <ul class="navbar-nav">
                            <li class="nav-item">
                                <a class="nav-link" asp-controller="home" asp-action="index">Home</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" asp-controller="auction" asp-action="index">Auction</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" asp-controller="aboutus" asp-action="index">About Us</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" asp-controller="contact" asp-action="index">Contact</a>
                            </li>
                        </ul>

                    </div>
                    <div class="search-nav">
                        <span class="icon">
                            <i class="fas fa-search"></i>
                        </span>
                        <form class="search-form my-2 my-lg-0">
                            <input class="form-control mr-sm-2" type="search" placeholder="Search for brand, model...."
                                   aria-label="Search">
                            <button class="btn-search my-2 my-sm-0" type="submit"><i class="fas fa-search"></i></button>
                        </form>
                    </div>
                </div>
            </nav>
        </div>
        <!-- End Navbar -->
    </header>

    <main>
        @RenderBody()
    </main>

    <footer style="background-image: url(/uploads/footer-bg.jpg);">
        <div class="footer-bg">
            <img src="~/uploads/footer-top-shape.png" class="img-fluid" alt="">
        </div>
        <div class="subscribe">
            <div class="container area">
                <div class="row">
                    <div class="col-lg-6">
                        <div class="image">
                            <img src="~/uploads/newslater.png" class="img-fluid" alt="">
                        </div>
                    </div>
                    <div class="col-lg-6">
                        <div class="content">
                            <div class="title">
                                <h4>Subscribe to Sbidu</h4>
                                <h2>To Get Exclusive Benefits</h2>
                            </div>
                            <form action="#">
                                <input type="text" placeholder="Enter Your Email">
                                <button type="submit">Subscribe</button>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <vc:footer-component></vc:footer-component>
        <div class="footer-bottom">
            <div class="container">
                <div class="copyright">
                    <img src="~/uploads/logo.png" class="logo" alt="">
                    <ul class="d-flex">
                        <li>
                            <img src="~/uploads/paypal.png" alt="">
                        </li>
                        <li>
                            <img src="~/uploads/visa.png" alt="">
                        </li>
                        <li>
                            <img src="~/uploads/discover.png" alt="">
                        </li>
                        <li>
                            <img src="~/uploads/mastercard.png" alt="">
                        </li>
                    </ul>
                    <p>
                        © Copyright 2020 |
                        <a href="#">Sbidu</a>
                        By
                        <a href="#">Uiaxis</a>
                    </p>
                </div>
            </div>
        </div>

    </footer>

    <div class="back-to-top">
        <i class="fas fa-angle-up"></i>
    </div>

    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/4.5.3/js/bootstrap.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery.countdown/2.2.0/jquery.countdown.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/wow/1.1.2/wow.min.js"></script>
    <script src="~/js/main.js"></script>
    @RenderSection("Scripts", required: false)
</body>
</html>